{
  "name": "dreamshot-model-swap",
  "private": true,
  "scripts": {
    "build": "bun run --env-file=.env prisma generate && bun run --env-file=.env prisma migrate deploy && remix vite:build",
    "dev": "./scripts/dev.sh",
    "dev:stable": "./scripts/dev-stable.sh",
    "update-domain": "./scripts/update-domain.sh",
    "config:link": "shopify app config link",
    "generate": "shopify app generate",
    "deploy": "shopify app deploy",
    "config:use": "shopify app config use",
    "env": "shopify app env",
    "start": "NODE_OPTIONS='--import ./instrumentation.server.mjs' remix-serve ./build/server/index.js",
    "docker-start": "bun run setup && bun run start",
    "setup": "prisma generate && prisma db push",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "shopify": "shopify",
    "prisma": "prisma",
    "graphql-codegen": "graphql-codegen",
    "vite": "vite",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f postgres",
    "db:psql": "docker exec -it dreamshot-postgres psql -U postgres -d dreamshot",
    "postinstall": "prisma generate",
    "vercel-build": "npx prisma generate && remix vite:build",
    "env:init": "bash ./scripts/init-env.sh",
    "migrate:dev": "bun run --env-file=.env prisma migrate dev",
    "migrate:deploy": "bun run --env-file=.env prisma migrate deploy",
    "db:seed": "bash -lc 'if [ -z \"$DATABASE_URL\" ]; then echo \"DATABASE_URL not set. Skipping seed.\"; else ts-node --transpile-only prisma/seed.ts || node --loader ts-node/esm prisma/seed.ts || node build/prisma/seed.js; fi'",
    "migrate:ai-studio": "bun run app/scripts/migrate-ai-studio-metafields.server.ts",
    "migrate:ai-studio:cleanup": "bun run app/scripts/migrate-ai-studio-metafields.server.ts --cleanup",
    "backfill:statistics": "bun run --env-file=.env scripts/backfill-daily-statistics.ts",
    "link:sessions": "bun run --env-file=.env scripts/link-sessions-to-credentials.ts",
    "activate:pixels": "bun run --env-file=.env scripts/activate-pixel-all-shops.ts"
  },
  "type": "module",
  "dependencies": {
    "@aws-sdk/client-s3": "3.879.0",
    "@aws-sdk/s3-request-presigner": "3.879.0",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@fal-ai/client": "1.6.2",
    "@iarna/toml": "^2.2.5",
    "@prisma/client": "^6.17.1",
    "@remix-run/dev": "2.16.7",
    "@remix-run/fs-routes": "2.16.7",
    "@remix-run/node": "2.16.7",
    "@remix-run/react": "2.16.7",
    "@remix-run/serve": "2.16.7",
    "@sentry/remix": "^10",
    "@sentry/vite-plugin": "^4.6.1",
    "@shopify/app-bridge-react": "4.2.7",
    "@shopify/cli": "3.84.1",
    "@shopify/polaris": "^12.0.0",
    "@shopify/polaris-icons": "^9.3.1",
    "@shopify/shopify-app-remix": "^3.7.0",
    "@shopify/shopify-app-session-storage-prisma": "^6.0.0",
    "@vercel/remix": "2.16.7",
    "framer-motion": "^12.23.24",
    "isbot": "^5.1.0",
    "prisma": "^6.2.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "replicate": "^1.0.3",
    "vite-tsconfig-paths": "^5.0.1"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "2.16.7",
    "@remix-run/route-config": "2.16.7",
    "@shopify/api-codegen-preset": "^1.1.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@vitest/ui": "^2.1.8",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^10.0.1",
    "jsdom": "^27.2.0",
    "prettier": "^3.2.4",
    "terser": "^5.44.0",
    "typescript": "^5.2.2",
    "vite": "^6.2.2",
    "vitest": "^2.1.8"
  },
  "workspaces": [
    "extensions/*"
  ],
  "trustedDependencies": [
    "@shopify/plugin-cloudflare"
  ],
  "resolutions": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5",
    "vite": "^6.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "overrides": {
    "@graphql-tools/url-loader": "8.0.16",
    "@graphql-codegen/client-preset": "4.7.0",
    "@graphql-codegen/typescript-operations": "4.5.0",
    "minimatch": "9.0.5",
    "vite": "^6.2.2",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "author": "javierjrueda"
}